<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane stylesheets="@../css/finalscene.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="lost_pane" alignment="TOP_CENTER" disable="true" spacing="100.0" StackPane.alignment="CENTER">
         <children>
            <Text fill="#d30000" stroke="#fff6f6" strokeType="OUTSIDE" text="YOU LOST!" textAlignment="CENTER">
               <VBox.margin>
                  <Insets top="300.0" />
               </VBox.margin>
               <font>
                  <Font name="Lato Black" size="90.0" />
               </font>
            </Text>
            <Text fill="#0a0a0a" stroke="#fff6f6" strokeType="OUTSIDE" text="You can't beat the Master of the Renaissance" textAlignment="CENTER">
               <font>
                  <Font name="Lato Black" size="87.0" />
               </font>
            </Text>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <VBox fx:id="won_pane" alignment="TOP_CENTER" disable="true" spacing="100.0" StackPane.alignment="CENTER">
         <children>
            <Text fill="#16db5d" stroke="#1b1818" strokeType="OUTSIDE" text="YOU WON!" textAlignment="CENTER">
               <VBox.margin>
                  <Insets top="300.0" />
               </VBox.margin>
               <font>
                  <Font name="Lato Black" size="87.0" />
               </font>
            </Text>
            <Text fill="#0a0a0a" stroke="#fff6f6" strokeType="OUTSIDE" text="You are the Master of Renaissance" textAlignment="CENTER">
               <font>
                  <Font name="Lato Black" size="87.0" />
               </font>
            </Text>
            <Text fx:id="score" stroke="#fff6f6" strokeType="OUTSIDE" text="" textAlignment="CENTER" wrappingWidth="706.064453125">
               <font>
                  <Font size="70.0" />
               </font>
            </Text>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <VBox fx:id="endgame_pane" alignment="TOP_CENTER" spacing="100.0" StackPane.alignment="CENTER">
         <children>
            <Text fx:id="game_over" fill="#2566d0" stroke="#bfb8b8" strokeType="OUTSIDE" text="The game has ended! These are the rankings:" textAlignment="CENTER">
               <VBox.margin>
                  <Insets top="150.0" />
               </VBox.margin>
               <font>
                  <Font name="Lato Black" size="65.0" />
               </font>
            </Text>
            <VBox fx:id="rank_pane" alignment="TOP_CENTER" prefHeight="1020.0" prefWidth="736.0" spacing="50.0">
               <children>
                  <Text stroke="#fff6f6" strokeType="OUTSIDE" text="1. ">
                     <font>
                        <Font name="Lato Black" size="67.0" />
                     </font>
                  </Text>
                  <Text stroke="#fff6f6" strokeType="OUTSIDE" text="2. ">
                     <font>
                        <Font name="Lato Black" size="67.0" />
                     </font>
                  </Text>
                  <Text stroke="#fff6f6" strokeType="OUTSIDE" text="3. ">
                     <font>
                        <Font name="Lato Black" size="67.0" />
                     </font>
                  </Text>
                  <Text stroke="#fff6f6" strokeType="OUTSIDE" text="4. ">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <font>
                        <Font name="Lato Black" size="67.0" />
                     </font>
                  </Text>
               </children>
               <padding>
                  <Insets top="50.0" />
               </padding>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <VBox alignment="BOTTOM_CENTER" StackPane.alignment="BOTTOM_CENTER">
         <children>
            <Text fill="#52a7eb" stroke="BLACK" strokeType="OUTSIDE" text="Thanks for playing with us!" textAlignment="CENTER" wrappingWidth="954.347412109375">
               <font>
                  <Font name="Lato Black" size="77.0" />
               </font>
            </Text>
         </children>
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
